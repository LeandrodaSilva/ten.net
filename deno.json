{
  "name": "@leproj/tennet",
  "version": "0.1.22",
  "license": "MIT",
  "exports": {
    ".": "./src/mod.ts",
    "./route-factory": "./src/routeFactory.ts"
  },
  "tasks": {
    "dev": "deno run -P --inspect --unstable-bundle --unstable-raw-imports --watch demo.ts",
    "test": "deno test --allow-all --unstable-raw-imports",
    "fmt": "deno fmt",
    "lint": "deno lint",
    "check": "deno check **/*.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@^1.0.15",
    "deno-fs": "jsr:@std/fs@^1.0.19",
    "@std/path": "jsr:@std/path@^1.1.2",
    "@deno-assert": "https://deno.land/std@0.208.0/assert/mod.ts",
    "@deno-walk": "jsr:@std/fs@1/walk",
    "react": "npm:react@^19.1.1",
    "react-dom": "npm:react-dom@^19.1.1",
    "react-dom/server": "npm:react-dom@^19.1.1/server"
  },
  "exclude": [
    ".git",
    "node_modules",
    "dist",
    "build"
  ],
  "permissions": {
    "default": {
      "read": true,
      "write": true,
      "env": true,
      "net": true
    }
  },
  "publish": {
    "include": [
      "src/",
      "README.md",
      "LICENSE",
      "deno.json"
    ]
  }
}
